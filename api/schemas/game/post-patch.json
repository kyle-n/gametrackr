{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "deck",
    "description",
    "expectedReleaseDay",
    "expectedReleaseMonth",
    "expectedReleaseYear",
    "gbId",
    "image",
    "name",
    "platforms"
  ],
  "properties": {
    "deck": {
      "$id": "#/properties/deck",
      "type": "string",
      "title": "The Deck Schema",
      "default": "",
      "examples": [
        "Example deck"
      ],
      "pattern": "^(.*)$"
    },
    "description": {
      "$id": "#/properties/description",
      "type": "string",
      "title": "The Description Schema",
      "default": "",
      "examples": [
        "Example description"
      ],
      "pattern": "^(.*)$"
    },
    "developers": {
      "$id": "#/properties/developers",
      "type": "array",
      "title": "The Developers Schema",
      "items": {
        "$id": "#/properties/developers/items",
        "type": "string",
        "title": "The Items Schema",
        "default": "",
        "examples": [
          "Example developer"
        ],
        "pattern": "^(.*)$"
      }
    },
    "expectedReleaseDay": {
      "$id": "#/properties/expectedReleaseDay",
      "type": "integer",
      "title": "The Expectedreleaseday Schema",
      "default": 0,
      "examples": [
        1
      ]
    },
    "expectedReleaseMonth": {
      "$id": "#/properties/expectedReleaseMonth",
      "type": "integer",
      "title": "The Expectedreleasemonth Schema",
      "default": 0,
      "examples": [
        1
      ]
    },
    "expectedReleaseYear": {
      "$id": "#/properties/expectedReleaseYear",
      "type": "integer",
      "title": "The Expectedreleaseyear Schema",
      "default": 0,
      "examples": [
        2019
      ]
    },
    "franchises": {
      "$id": "#/properties/franchises",
      "type": "array",
      "title": "The Franchises Schema",
      "items": {
        "$id": "#/properties/franchises/items",
        "type": "string",
        "title": "The Items Schema",
        "default": "",
        "examples": [
          "Example franchise"
        ],
        "pattern": "^(.*)$"
      }
    },
    "genres": {
      "$id": "#/properties/genres",
      "type": "array",
      "title": "The Genres Schema",
      "items": {
        "$id": "#/properties/genres/items",
        "type": "string",
        "title": "The Items Schema",
        "default": "",
        "examples": [
          "Example genre"
        ],
        "pattern": "^(.*)$"
      }
    },
    "gbId": {
      "$id": "#/properties/gbId",
      "type": "integer",
      "title": "The Gbid Schema",
      "default": 0,
      "examples": [
        1
      ]
    },
    "image": {
      "$id": "#/properties/image",
      "type": "string",
      "title": "The Image Schema",
      "default": "",
      "examples": [
        "https://www.giantbomb.com"
      ],
      "pattern": "^(.*)$"
    },
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The Name Schema",
      "default": "",
      "examples": [
        "Example custom game"
      ],
      "pattern": "^(.*)$"
    },
    "platforms": {
      "$id": "#/properties/platforms",
      "type": "array",
      "title": "The Platforms Schema",
      "items": {
        "$id": "#/properties/platforms/items",
        "type": "integer",
        "title": "The Items Schema",
        "default": 0,
        "examples": [
          1
        ]
      }
    }
  }
}