{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "POST User",
  "required": [
    "name",
    "password",
    "email"
  ],
  "properties": {
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "minLength": 1,
      "maxLength": 32,
      "title": "The Name Schema",
      "default": "",
      "examples": [
        "Kyle"
      ],
      "pattern": ".{1,}"
    },
    "password": {
      "$id": "#/properties/password",
      "type": "string",
      "minLength": 1,
      "maxLength": 10000,
      "title": "The Password Schema",
      "default": "",
      "examples": [
        "pw"
      ],
      "pattern": ".{1,}"
    },
    "email": {
      "$id": "#/properties/email",
      "type": "string",
      "format": "email",
      "title": "The Email Schema",
      "default": "",
      "examples": [
        "kyle@x.com"
      ]
    }
  }
}