{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "name",
    "deck",
    "original_release_date",
    "image",
    "lists"
  ],
  "properties": {
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The Name Schema",
      "default": "",
      "examples": [
        "123123123123123123"
      ],
      "pattern": "^.{0,120}$"
    },
    "deck": {
      "$id": "#/properties/deck",
      "type": "string",
      "title": "The Deck Schema",
      "default": "",
      "examples": [
        "7329873297459837459837345345"
      ],
      "pattern": "^.{0,240}$"
    },
    "original_release_date": {
      "$id": "#/properties/original_release_date",
      "type": "string",
      "title": "The Original_release_date Schema",
      "default": "",
      "examples": [
        "2008-12-21"
      ],
      "pattern": "^.{0,40}$"
    },
    "image": {
      "$id": "#/properties/image",
      "type": "string",
      "title": "The Image Schema",
      "default": "",
      "examples": [
        "http://i.imgur.com/G1CdQJz.png"
      ],
      "pattern": "^.*$"
    },
    "lists": {
      "$id": "#/properties/lists",
      "type": "array",
      "title": "The Lists Schema",
      "items": {
        "$id": "#/properties/lists/items",
        "type": "integer",
        "title": "The Items Schema",
        "default": 0,
        "examples": [
          1,
          2
        ],
        "minItems": 1
      }
    }
  }
}