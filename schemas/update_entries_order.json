{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "entries"
  ],
  "properties": {
    "entries": {
      "$id": "#/properties/entries",
      "type": "array",
      "title": "The Entries Schema",
      "items": {
        "$id": "#/properties/entries/items",
        "type": "object",
        "title": "The Items Schema",
        "required": [
          "id",
          "ranking"
        ],
        "properties": {
          "id": {
            "$id": "#/properties/entries/items/properties/id",
            "type": "integer",
            "title": "The Id Schema",
            "default": 0,
            "examples": [
              1
            ]
          },
          "ranking": {
            "$id": "#/properties/entries/items/properties/ranking",
            "type": "integer",
            "title": "The Ranking Schema",
            "default": 0,
            "examples": [
              2
            ]
          }
        }
      }
    }
  }
}