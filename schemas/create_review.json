      "minimum": 0,
      "examples": [
        1
      ]
    },
    "stars": {
      "$id": "#/properties/stars",
      "type": "number",
      "title": "The Stars Schema",
      "default": 0,
      "minimum": 0,
      "maximum": 5,
      "examples": [
        2
      ]
    }
  }
}