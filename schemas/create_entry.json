{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "game_id",
    "text"
  ],
  "properties": {
    "game_id": {
      "$id": "#/properties/game_id",
      "type": "integer",
      "title": "The Game_id Schema",
      "default": 0,
      "examples": [
        1
      ]
    },
    "text": {
      "$id": "#/properties/text",
      "type": "string",
      "title": "The Text Schema",
      "default": "",
      "examples": [
        "Game entry text"
      ],
      "pattern": "^.{0,240}$"
    },
    "ranking": {
      "$id": "#/properties/ranking",
      "type": "integer",
      "title": "The Ranking Schema",
      "default": 1,
      "examples": [
        2
      ],
      "minimum": 1
    }
  }
}